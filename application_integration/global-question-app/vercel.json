{
  "crons": [
    {
      "path": "/api/push-window-open",
      "schedule": "*/5 * * * *"
    },

    {
      "path": "/api/cron/daily-aggregate",
      "schedule": "5 * * * *"
    },
    {
      "path": "/api/cron/daily-users",
      "schedule": "10 * * * *"
    },
    {
      "path": "/api/cron/daily-cohorts",
      "schedule": "15 * * * *"
    },
    {
      "path": "/api/cron/daily-badges",
      "schedule": "20 * * * *"
    },
    {
      "path": "/api/cron/daily-push",
      "schedule": "25 * * * *"
    },
    {
      "path": "/api/end-day",
      "schedule": "55 23 * * *"
    }
  ],

  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
