{
  "crons": [
    {
      "path": "/api/push-window-open",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/cron/daily-aggregate",
      "schedule": "0 * * * *"
    }
  ],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
